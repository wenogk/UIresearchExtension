{
  "name": "UI Research Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "UI Research Extension",
  "homepage_url": "http://wenogk.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage"
  ]
}